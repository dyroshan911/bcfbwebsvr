<link href="stylesheets/login.css" rel="stylesheet" />
<div class="loginpage">
	<div class="loginbox">
		<div class="loginboxinner">
			<div class="logo">
				<h1 class="logo">
					百城.<span>金融</span>
				</h1>
				<span class="slogan">后台管理系统</span>
			</div>
			<!--logo-->
			<br clear="all" />
			<br />
			<div class="nousername">
				<div class="loginmsg">密码不正确.</div>
			</div>
			<!--nousername-->
			<div class="nopassword">
				<div class="loginmsg">密码不正确.</div>
				<div class="loginf">
					<div class="thumb">
						<img alt="" src="images/thumbs/avatar1.png" />
					</div>
					<div class="userlogged">
						<h4></h4>
						<a href="index.html">
							Not <span></span>?
						</a>
					</div>
				</div>
				<!--loginf-->
			</div>
			<!--nopassword-->
			<form id="login" name='loginForm' role="form" ng-submit="onLogin()">
				<div class="username">
					<div class="usernameinner">
						<input type="text" name="username" id="username" required="" maxlength="20" ng-model="loginData.userName" />
					</div>
				</div>
				<div class="password">
					<div class="passwordinner">
						<input type="password" name="password" id="password" required="" maxlength="20" ng-model="loginData.password" />
					</div>
				</div>
				<button type="submit">登录</button>
				<div class="keep">
					<input type="checkbox" disabled="" />
				</div>
			</form>
		</div>
		<!--loginboxinner-->
	</div>
	<!--loginbox-->
</div>