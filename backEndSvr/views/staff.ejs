<div class ="container-fluid" ng-controller="StaffCtrl">
	<br/>
	<div class="input-group col-xs-5 col-sm-4 col-md-3 col-lg-2">
		<input type="text" class="form-control" placeholder="搜索" maxlength="10" ng-model="search" />
		<span class="input-group-addon">
			<a href="#" class="glyphicon glyphicon-search" ng-click="onSearch()"></a>
		</span>
	</div>
	<br/>
	<div class="row">
		<table class="table table-bordered table-striped table-hover">
			<thead>
				<tr>
					<th width="10%">账号</th>
					<th width="10%">姓名</th>
					<th width="10%">工号</th>
					<th width="10%">身份</th>
					<th width="10%">电话</th>
					<th width="15%">注册日期</th>
					<th width="10%">上级ID</th>
					<th width="5%"></th>
				</tr>
			</thead>
			<tbody>
				<tr ng-repeat="channel in channelList">
					<td align="center" ng-bind="channel.id"></td>
					<td align="center" ng-bind="channel.true_name"></td>
					<td align="center" ng-bind="channel.job_number"></td>
					<td align="center" ng-bind="channel.roleName"></td>
					<td align="center" ng-bind="channel.phone"></td>
					<td align="center" ng-bind="channel.createDate"></td>
					<td align="center" ng-bind="channel.superior"></td>
					<td align="center">
						<a href="#">修改</a>
					</td>
				</tr>
			</tbody>
		</table>
	</div>
	<nav>
		<ul class="pagination">
			<li ng-repeat="page in pageData.list">
				<a href="#" ng-class="{true: 'currentPageStyle'}[page == pageData.current]" ng-bind="page.name" ng-click="gotoPage(pageData, page.index)"></a>
			</li>
		</ul>
	</nav>
</div>